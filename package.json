{
  "private": true,
  "name": "ftson",
  "version": "0.0.0",
  "description": "Fast JSON powered by TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/basic-examples/ftson.git"
  },
  "bin": {
    "ftson": "./bin.js"
  },
